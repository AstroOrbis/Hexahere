{
  "schemaVersion": 1,
  "id": "hexahere",
  "version": "${version}",
  "name": "Hexahere",
  "description": "small utils for hexcasting",
  "authors": [
    "AstroOrbis"
  ],
  "contact": {
    "homepage": "https://astroorbis.github.io/Hexahere",
    "sources": "https://github.com/AstroOrbis/Hexahere"
  },
  "license": "MIT",
  "icon": "assets/hexahere/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.astroorbis.hexahere.fabric.FabricHexahere"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.astroorbis.hexahere.fabric.FabricHexahereClient"
      }
    ],
    "server": [
      {
        "adapter": "kotlin",
        "value": "com.astroorbis.hexahere.fabric.FabricHexahereServer"
      }
    ],
    "fabric-datagen": [
      {
        "adapter": "kotlin",
        "value": "com.astroorbis.hexahere.fabric.datagen.FabricHexahereDatagen"
      }
    ],
    "modmenu": [
      {
        "adapter": "kotlin",
        "value": "com.astroorbis.hexahere.fabric.FabricHexahereModMenu"
      }
    ]
  },
  "mixins": [
    "hexahere-common.mixins.json",
    "hexahere-fabric.mixins.json"
  ],
  "depends": {
    "minecraft": "${versions.minecraft}",
    "fabricloader": ">=${versions.fabric_loader}",
    "fabric-api": ">=${versions.fabric_api}",
    "fabric-language-kotlin": ">=${versions.kotlin_fabric}",
    "architectury": ">=${versions.architectury}",
    "hexcasting": ">=${versions.hexcasting_dependency}",
    "cloth-config": ">=${versions.clothConfig}"
  },
  "suggests": {
    "modmenu": ">=${versions.modMenu}"
  }
}
